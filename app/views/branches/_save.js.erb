$("ul.errors").html("")
<% if @branch.errors.any? %>
  <% @branch.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>

<!-- $(".user-index").html("<%= j render 'index' %>") -->
  $("#branch-modal").modal("hide")
<% end %>