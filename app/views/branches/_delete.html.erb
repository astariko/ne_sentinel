<div class="modal-dialog">
  <div class="modal-content">
  <div class="modal-header">
    <h3>Delete Branch</h3>
  </div>
  <div class="modal-body">
  <ul class="errors"></ul>
  Are you sure you wish to delete <b><%= @branch.name %></b>?
  </div>
  <div class="modal-footer">
    <%= link_to "Confirm", user_branch_path(@user, @branch), method: :delete, class: "btn btn-primary" %>
    <%= link_to "Cancel", "#", class: "btn btn-default", data: { dismiss: "modal" } %>
  </div>
  </div>
</div>